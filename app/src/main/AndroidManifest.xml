<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.maanjo.sqlite_diabetesapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Activities.logIn"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activities.startingPage"
            android:label="@string/title_activity_starting_page" >
        </activity>
        <activity
            android:name=".Activities.registrate"
            android:label="@string/title_activity_registrate"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
            android:name=".Activities.tableView"
            android:label="@string/title_activity_tableView"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
    </application>

</manifest>